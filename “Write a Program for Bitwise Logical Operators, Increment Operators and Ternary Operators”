“Write a Program for Bitwise Logical Operators, Increment Operators and Ternary Operators”






#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for (int i=0;i<t;i++){
	     int a,b;
	     cin>>a>>b;
	     if(a>b){
	          cout<<a<<'>'<<b<<endl;
	     }
	     else if(b>a){
	          cout<<a<<'<'<<b<<endl;
	     }
	     else{
	          cout<<'='<<endl;
	     }
	}
	return 0;
}







/*
INPUT
5
6

OUTPUT
5<6
*/
